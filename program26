list1=list()
list2=list()
tuple1=tuple()
n=int(input("enter  no.of elements in a tuple: "))
for i in range(n):
    list1.append(int(input("enter element {} in tuple: ".format(i+1))))
tuple1=tuple(list1)
print(tuple1)
for i in tuple1:
    cnt=tuple1.count(i)
    if cnt>1:
        list2.append(i)
set1=tuple(set(list2))
print("repeated elements: ",set1)
