import cmath
import math
a=int(input("enter x^2 coefficient:"))
b=int(input("enter x coeficient:"))
c=int(input("enter constamt:"))
print("the quadratic equation is: ",a,"x^2+",b,"x+",c,"=0")
d=math.pow(b,2)-4*a*c
print("one root is: ",-b-cmath.sqrt(d)/(2*a))
print("another root is: ",-b+cmath.sqrt(d)/(2*a))
